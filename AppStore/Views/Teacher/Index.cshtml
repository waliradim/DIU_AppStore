@model IEnumerable<AppStore.Models.VMTeacherCourse>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TeacherLayoutPage.cshtml";
}

<h2>Your Courses</h2>

<table class="table">
    <tr>
        
        <th>
            Teacher Name
        </th>
        <th>
            Course Name
        </th>
        <th>
            Semester Name
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        
        <td>
            @item.TeacherName
        </td>
        <td>
            @item.CourseName
        </td>
        <td>
            @item.SemesterName
        </td>
    </tr>
    
}

</table>
